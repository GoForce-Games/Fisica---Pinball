<config>
  <app>
    <title>Game Development template</title>
    <maxFrameDuration value="16"/>
  </app>

  <renderer>
    <vsync value="true"/>
  </renderer>

  <window>
    <resolution width="350" height="640" scale="1"/>
    <fullscreen value="false"/>
    <borderless value="false"/>
    <resizable value="false"/>
    <fullscreen_window value="false"/>
  </window>

  <audio>
    <music volume="256"/>
    <fx volume="256"/>
  </audio>

  <scene>
    <map path="Assets/Maps/" name="Pinball.tmx"/>
  </scene>

	<scoreModule path="scores.xml"/>

	<entitymanager>
		<entitypresets>
			<bumper x="160" y ="160" radius="32" texturepath="Assets/Textures/bumper.png" launchPower="0.4f" pointValue="100"/>
			<cannon x="316" y="560" texturepath="Assets/Textures/player1.png" launchAngle="90.0f" launchPowerIncrease="0.01f" launchPowerCap="30.0f"/>
			<ball x="316" y="560" radius="4" texturepath= "Assets/Textures/pinball.png"/>
			<palas x1="65" x2="172" y1="495" y2="495" texturepath1="Assets/Textures/Pala izquierda.png" texturepath2="Assets/Textures/Pala derecha.png" angle1="0.0f" angle2="0.0f"/>
		</entitypresets>
	</entitymanager>
  
</config>
